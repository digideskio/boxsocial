<%
if (guests.length == 0) {
    %>
    <li>No one</li>
    <%
}
for (guestIndex in guests) {
    %>
    <li class="guest">
        <%
        if (guests[guestIndex].user && guests[guestIndex].user.image[1]["#text"]) {
            %>
            <img src="<%= guests[guestIndex].user.image[1]["#text"] %>" />
            <%
        }
        %>
        <div class="name">
            <a href="http://www.last.fm/user/<%=guests[guestIndex].session.user%>"><%= guests[guestIndex].session.user %></a>
        </div>
    </li>
    <%
}
%>
