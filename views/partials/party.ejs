<div class="host">
    <a href="/party/<%= party.host %>"><%= party.host %>'s party</a>
</div>
<div class="track">
    <%
    if (party.nowPlaying) {
        %>
        <%- partial("track", party.nowPlaying) %>
        <%
    }
    else {
        %>
        <%- partial("nothing") %>
        <%
    }
    %>
</div>
